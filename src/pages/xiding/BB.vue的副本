<template>
	<div>
		<div class="hello" @click="bbclick">1111 ---- {{ number }}</div>

	</div>
</template>
<script>
export default {
  data() {
    return {
    	number:'0'
    }
  },
  methods:{
  		bbclick(){
  			this.number ++
  			this.$emit('change')
  		}
  }
}

</script>
